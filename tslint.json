{
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "rules": {
    "import-name": [false],
    "member-access": false,
    "no-any": true,
    "no-console": false,
    "object-literal-sort-keys": false,
    "prefer-array-literal": [true, { "allow-type-parameters": true }],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  }
}
